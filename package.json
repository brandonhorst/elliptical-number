{
  "name": "lacona-phrase-number",
  "version": "0.1.0",
  "description": "Enable Lacona to parse numbers, including integers, floats, ane more",
  "main": "lib/number.js",
  "scripts": {
    "lint": "npm run lint:lib && npm run lint:test",
    "lint:lib": "jshint --config node_modules/lacona-util-jshint/lib.json lib",
    "lint:test": "jshint --config node_modules/lacona-util-jshint/test.json test",
    "test": "mocha test",
    "validate": "npm run lint && npm test",
    "cover": "istanbul cover _mocha test -- -R dot && istanbul report text",
    "coveralls": "istanbul cover _mocha test --report lcovonly -- -R dot && cat coverage/lcov.info | coveralls && rimraf ./coverage",
    "prepublish": "npm run validate"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brandonhorst/lacona-phrase-datetime.git"
  },
  "keywords": [
    "lacona",
    "number",
    "float",
    "integer"
  ],
  "author": "@brandonhorst",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brandonhorst/lacona-phrase-number/issues"
  },
  "homepage": "https://github.com/brandonhorst/lacona-phrase-number",
  "devDependencies": {
    "array-stream": "^0.1.1",
    "chai": "^1.10.0",
    "coveralls": "^2.11.2",
    "event-stream": "^3.2.1",
    "istanbul": "^0.3.5",
    "jshint": "^2.6.0",
    "lacona-util-fulltext": "^0.2.0",
    "lacona-util-jshint": "^0.2.0",
    "mocha": "^2.1.0",
    "stream-array": "^1.0.1"
  },
  "dependencies": {
    "lacona": "^0.16.0",
    "lacona-phrase-freetext": "^0.5.2"
  }
}
